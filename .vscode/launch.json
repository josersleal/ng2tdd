{
  "version": "0.2.0",
  "configurations": [{
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceRoot}"
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach Karma to Chrome",
      "port": 9332,
      "pathMapping": {
        "/": "${workspaceRoot}",
        "/base/": "${workspaceRoot}"
      }
    },
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach Karma to Chrome2",
      "url": "",
      "port": 9333,
      "webRoot": "${workspaceRoot}"
    }
  ]
}
